{
  "name": "scheduled-affirmations",
  "version": "1.0.3",
  "main": "expo-router/entry",
  "scripts": {
    "start": "npx expo start --dev-client",
    "android": "npx expo run:android",
    "ios": "npx expo run:ios",
    "lint": "npx eslint src/**/* --fix",
    "web": "expo start --web",
    "podpod": "cd ios && pod install && cd ../",
    "nuke": "rm -rf node_modules",
    "prepare": "husky install",
    "nukestart": "yarn nuke && yarn && podpod && npx expo install && yarn ios;",
    "eas.build": "eas build",
    "full.build": "nuke;yarn nuke;eas build;yarn ios",
    "submit.build": "eas build --auto-submit",
    "version": "eas build:version:set",
    "tc": "npx tsc"
  },
  "dependencies": {
    "@expo/vector-icons": "13.0.0",
    "@gorhom/bottom-sheet": "4.6.1",
    "@react-native-async-storage/async-storage": "1.21.0",
    "@react-native-community/datetimepicker": "7.6.2",
    "@rneui/base": "4.0.0-rc.8",
    "@rneui/themed": "4.0.0-rc.8",
    "@shopify/react-native-skia": "1.2.3",
    "expo": "49.0.11",
    "expo-calendar": "11.3.2",
    "expo-constants": "14.4.2",
    "expo-dev-client": "2.4.11",
    "expo-device": "5.4.0",
    "expo-linking": "5.0.2",
    "expo-notifications": "0.20.1",
    "expo-router": "2.0.0",
    "expo-splash-screen": "0.20.5",
    "expo-status-bar": "1.6.0",
    "expo-updates": "0.18.16",
    "react": "18.2.0",
    "react-native": "0.72.6",
    "react-native-gesture-handler": "2.12.1",
    "react-native-reanimated": "3.11.0",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "3.22.1",
    "react-native-simple-toast": "3.1.0",
    "react-native-vision-camera": "4.0.3",
    "react-native-worklets-core": "1.3.2"
  },
  "devDependencies": {
    "@babel/core": "~7.22.19",
    "@types/react": "~18.2.48",
    "@types/url-parse": "~1.4.11",
    "@welldone-software/why-did-you-render": "~7.0.1",
    "eslint": "8.56.0",
    "eslint-config-kentcdodds": "~20.5.0",
    "eslint-config-next": "13.4.5",
    "eslint-config-prettier": "~8.10.0",
    "eslint-plugin-react": "~7.33.2",
    "eslint-plugin-react-hooks": "~4.6.0",
    "husky": "~8.0.3",
    "lint-staged": "~13.3.0",
    "prettier": "~2.8.8",
    "prettier-eslint": "~15.0.1",
    "prettier-plugin-organize-imports": "~3.2.2",
    "typescript": "~5.3.3"
  },
  "private": true
}
